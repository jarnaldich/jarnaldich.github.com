---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  
  <title>Joan Arnaldich Homepage</title>
  <link href="http://jarnaldich.net/feed.xml" rel="self"/>
  <link href="http://jarnaldich.net/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.jarnaldich.net/</id>
  <author>
    <name>Joan Arnaldich</name>
    <email>jarnaldich@google.com</email>
  </author>
  
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://jarnaldich.net{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://jarnaldich.net{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
  
</feed>
